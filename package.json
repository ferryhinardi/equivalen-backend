{
  "name": "equivalent",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "./node_modules/.bin/jest --maxWorkers=1",
    "start": "npm run build && npm run start:prod",
    "start:prod": "DEBUG=app* node ./dist/index.js",
    "start:dev": "npm run-script database:init && nodemon -e js,gql --exec DEBUG=app* babel-node ./src/index.js",
    "build": "npm run build:clean && npm run build:dist",
    "build:clean": "rimraf dist/*",
    "build:dist": "babel src --out-dir dist --copy-files --ignore *.test.js,__tests__,__mocks__",
    "build:watch": "npm run build:dist -- -w",
    "database:init": "mkdir -p dist/migrations src/migrations && npm run sequelize db:migrate",
    "sequelize": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize $*"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "apollo-server": "^2.0.8",
    "apollo-server-express": "^2.0.7",
    "awesome-phonenumber": "^2.2.6",
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "continuation-local-storage": "^3.2.1",
    "cors": "^2.8.4",
    "crypto-js": "^3.1.9-1",
    "dataloader-sequelize": "^1.7.2",
    "debug": "^4.1.0",
    "decamelize": "^2.0.0",
    "dotenv": "^4.0.0",
    "envsub": "^3.1.0",
    "express": "^4.16.3",
    "faker": "^4.1.0",
    "glob": "^7.1.2",
    "graphql": "^14.0.2",
    "graphql-constraint-directive": "^1.2.0",
    "graphql-extensions": "^0.4.1",
    "graphql-relay": "^0.5.5",
    "graphql-sequelize": "9.2.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "merge-graphql-schemas": "^1.5.3",
    "moment": "^2.21.0",
    "mustache": "^3.0.1",
    "mysql2": "^1.5.1",
    "rimraf": "^2.6.2",
    "sequelize": "4.41.2",
    "sequelize-cli": "4.1.1",
    "sequelize-fixtures": "^0.7.0",
    "sqlite3": "^3.1.8",
    "superagent": "^3.8.3",
    "supertest": "^3.0.0",
    "uuid": "^3.3.2",
    "winston": "2.2.0",
    "winston-papertrail": "1.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "babel-jest": "23.4.2",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.21.0",
    "jest": "22.4.4",
    "nodemon": "^1.18.3"
  },
  "engines": {
    "node": ">=8.11.2 <=9.10.0",
    "npm": "5.x"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./jest/setup.js",
    "testEnvironment": "node",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
